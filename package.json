{
  "name": "@hyperwatch/useragent",
  "description": "User Agent parser",
  "license": "Apache-2.0",
  "author": "Fran√ßois Hodierne <francois@hodierne.net>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperwatch/useragent.git"
  },
  "version": "3.8.2",
  "main": "src/useragent.js",
  "scripts": {
    "depcheck": "depcheck",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix",
    "test": "mocha --recursive",
    "prettier": "prettier \"**/*.@(js|json|md|yml)\"",
    "prettier:check": "npm run prettier -- --list-different",
    "prettier:write": "npm run prettier -- --write",
    "update": "node scripts/update && prettier data/* --write"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "depcheck": "^1.3.1",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^11.1.1",
    "mocha": "^9.0.2",
    "node-fetch": "^3.0.0",
    "prettier": "^2.2.0",
    "prettier-package-json": "^2.1.3",
    "yamlparser": "0.0.2"
  },
  "engines": {
    "node": ">=12.0"
  },
  "depcheck": {
    "ignores": [
      "prettier-package-json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,yml}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier-package-json --write"
    ]
  }
}
